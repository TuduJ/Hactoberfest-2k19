
#include <stdio.h>

int main(){
	int n,i,j;
	scanf("%d",&n);
	
	 for(i=1;i<=((n*3)+2);i++)
	{
	   for(j=1;j<=5;j++)
	 { 
	     {
	   if(j==1||j==5)
	   { 
	       printf("*");
	   }
	   else if((i%3==0)&&(j!=1&&j!=(n+1)))
	   {
	    printf("*");
	   }
	   
	   else
	     printf(" ");
	 }
	}
	   printf("\n");
	}
	 return 0;
	
}
